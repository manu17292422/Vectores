#include <iostream>
using namespace std;
int main()
{  int n,temp;
cout<<"ingrese la cantidad de elemetnos: ";
cin>>n;
int num[n];
for(int i=0;i<n;i++)
{
    cout<<"Digite el numero: ";
    cin>>num[i];
}
temp=num[n-1];
for(int i=n-1;i>0;i--)
{
    num[i]=num[i-1];
}
num[0]=temp;
for(int i=0;i<n;i++)
{
    cout<<num[i]<<" ";
}

return 0;

}
